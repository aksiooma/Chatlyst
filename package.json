{
  "name": "chatbot-ai",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "concurrently \"tsc && vite build\" \"npm run build:server\"",
    "build:server": "cd server && npm run build",
    "start": "concurrently \"npm run dev\" \"npm --prefix ./Server run dev:server\"",
    "start:prod:server": "node server.js",
    "start:prod": "concurrently \"npm run start:prod:server\" \"serve -s dist\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express-session": "^1.17.8",
    "@types/styled-components": "^5.1.28",
    "axios": "^1.5.1",
    "express-session": "^1.17.3",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-textarea-autosize": "^8.5.3",
    "styled-components": "^6.0.8",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.7.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "nodemon": "^3.0.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-qrcode": "^0.2.2"
  }
}
